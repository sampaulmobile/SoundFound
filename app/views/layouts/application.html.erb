<!DOCTYPE html>
<html>
  <head>
    <title><%= full_title(yield(:title)) %></title>
    
    <%= stylesheet_link_tag    "application", "jquery.jqplot.min", media: "all", "data-turbolinks-track" => true %>
    <%= javascript_include_tag "application", "connect", "data-turbolinks-track" => true %>
    <%= javascript_include_tag "jquery.jqplot.min", "jqplot.pointLabels.min", "jqplot.categoryAxisRenderer.min", "jqplot.barRenderer.min" %>
    <%= javascript_include_tag :defaults %>
    <%= csrf_meta_tags %>

    <%= render 'layouts/shim' %>
    <%= yield :head %>
    
  </head>
  <body>
    <%= render 'layouts/header' %>

    <%= render 'layouts/messages' %>
    <%= yield %>

    <%= render 'layouts/footer' %>
    <%= debug(params) if Rails.env.development? %>
  </body>
</html>

